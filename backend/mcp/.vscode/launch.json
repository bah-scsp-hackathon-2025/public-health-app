{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/backend/mcp",
            "python": "${workspaceFolder}/backend/mcp/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/backend/mcp"
            },
            "justMyCode": false,
            "stopOnEntry": false,
            "args": []
